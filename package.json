{
  "name": "advanced-boilerplate",
  "version": "0.5.3",
  "description": "React Universally with PostCSS",
  "main": "es5/server/index.js",
  "jsnext:main": "src/server/index.js",
  "engines": {
    "node": "~6.3.0"
  },
  "scripts": {
    "dev": "npm run dev:start",
    "dev:start": "node ./devServer",
    "prod": "npm run prod:build && npm run prod:start",
    "prod:build": "npm run prod:clean && npm run prod:build:client && npm run prod:build:server",
    "prod:build:client": "webpack --env.mode production --config webpack.client.config.js",
    "prod:build:server": "webpack --env.mode production --config webpack.server.config.js",
    "prod:start": "node build/server/main.js",
    "prod:clean": "rimraf build",
    "prepublish": "npm run build-es5 && npm run build-es6",
    "build-es5": "babel --presets react,es2015,stage-1 --source-maps true --out-dir lib/ src/",
    "build-es6": "babel --presets react,es2015-webpack,stage-1 --source-maps true --out-dir es/ src/",
    "release": "release-it --github.release --npm.publish --non-interactive",
    "release:minor": "release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "release-it --github.release --npm.publish --non-interactive --increment major",
    "start": "npm run dev",
    "build": "npm run prod:build",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-software/advanced-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "universal",
    "javascript",
    "express",
    "webpack",
    "postcss",
    "cssmodules"
  ],
  "author": "Sebastian Software <s.werner@sebastian-software.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sebastian-software/advanced-boilerplate/issues"
  },
  "homepage": "https://github.com/sebastian-software/advanced-boilerplate#readme",
  "dependencies": {
    "assetgraph": "^2.13.0",
    "assets-webpack-plugin": "^3.4.0",
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015-webpack": "^6.4.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.5.0",
    "buble": "^0.12.5",
    "chokidar": "^1.6.0",
    "colorguard": "^1.2.0",
    "compression": "^1.6.2",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "cssstats": "^3.0.0-beta.1",
    "dotenv": "^2.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "helmet": "^2.1.2",
    "hpp": "^0.2.1",
    "json-loader": "^0.5.4",
    "load-plugins": "^2.1.0",
    "lost": "^7.0.3",
    "node-noop": "^1.0.0",
    "node-notifier": "^4.6.0",
    "normalize.css": "^4.2.0",
    "path": "^0.12.7",
    "postcss": "^5.1.1",
    "postcss-advanced-variables": "^1.2.2",
    "postcss-calc": "^5.3.0",
    "postcss-color-function": "^2.0.1",
    "postcss-color-hex-alpha": "^2.0.0",
    "postcss-devtools": "^1.1.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-easings": "^0.3.0",
    "postcss-flexbugs-fixes": "^2.0.0",
    "postcss-gradient-transparency-fix": "^1.0.2",
    "postcss-import": "8.0.x",
    "postcss-loader": "^0.9.1",
    "postcss-media-minmax": "^2.1.2",
    "postcss-nested": "^1.0.0",
    "postcss-pseudo-class-any-link": "^1.0.0",
    "postcss-pseudoelements": "^3.0.0",
    "postcss-reporter": "^1.4.1",
    "postcss-sassy-mixins": "^2.0.0",
    "postcss-selector-matches": "^2.0.1",
    "postcss-url": "^5.1.2",
    "postcss-will-change": "^1.1.0",
    "postcss-zindex": "^2.1.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-markdown": "^2.4.2",
    "react-router": "^2.6.1",
    "rimraf": "^2.5.4",
    "s15e-javascript": "^0.2.0",
    "serialize-javascript": "^1.3.0",
    "sinon": "^1.17.5",
    "source-map-support": "^0.4.2",
    "source-sans-pro": "^2.0.10",
    "style-loader": "^0.13.1",
    "stylefmt": "^4.2.1",
    "stylelint": "^7.0.3",
    "stylelint-config-standard": "^11.0.0",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-node-externals": "^1.3.3"
  }
}
